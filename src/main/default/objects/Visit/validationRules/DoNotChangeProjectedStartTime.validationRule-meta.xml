<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DoNotChangeProjectedStartTime</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  NOT(ISNEW()),
  NOT(ISBLANK(PRIORVALUE( ProjectedStartTime__c )))
)</errorConditionFormula>
    <errorMessage>You cannot edit Projected Start Time</errorMessage>
</ValidationRule>
